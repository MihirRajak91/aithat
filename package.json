{
  "name": "ai-plan",
  "displayName": "AI Plan",
  "description": "Generate implementation plans from recent tickets using AI",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-plan.generateFromRecent",
        "title": "Generate Plan from Recent Tickets",
        "category": "AI Plan"
      },
      {
        "command": "ai-plan.generateFromRecentStreaming",
        "title": "Generate Plan from Recent Tickets (Streaming)",
        "category": "AI Plan"
      },
      {
        "command": "ai-plan.configure",
        "title": "AI Plan: Configure",
        "category": "AI Plan"
      },
      {
        "command": "ai-plan.settings",
        "title": "AI Plan: Settings",
        "category": "AI Plan"
      },
      {
        "command": "ai-plan.openConfiguration",
        "title": "AI Plan: Open Configuration Panel",
        "category": "AI Plan"
      },
      {
        "command": "ai-plan.showStatus",
        "title": "AI Plan: Show Status",
        "category": "AI Plan"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ai-plan.generateFromRecent",
          "group": "navigation"
        },
        {
          "command": "ai-plan.generateFromRecentStreaming",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "node ./out/test/runTest.js",
    "test:unit": "mocha out/test/**/*.test.js",
    "clean": "rimraf out",
    "package": "vsce package",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.0.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ignore": "^5.3.0",
    "@slack/web-api": "^6.10.0",
    "@octokit/rest": "^20.0.0",
    "dotenv": "^16.3.1"
  }
}
